<?xml version='1.0' encoding='utf-8'?>
<manifest package="org.monarq.mtg.proLIFErate" android:versionName="0.2" xmlns:android="http://schemas.android.com/apk/res/android" android:versionCode="2">
    <uses-sdk android:minSdkVersion="10"/>
